{
  "Queued": {
    "get": {
      "description": "Returns an Array with all Tasks currently being scheduled",
      "responses": {
        "200": {
          "$ref": "../Api.json#/components/responses/200_TaskList"
        },
        "401": {
          "$ref": "../Api.json#/components/responses/401"
        }
      },
      "security": [
        {
          "basicAuth": []
        }
      ]
    }
  },
  "Dispatched": {
    "get": {
      "description": "Returns an Array with all Tasks currently waiting for feedback from dispatcher",
      "responses": {
        "200": {
          "$ref": "../Api.json#/components/responses/200_TaskList"
        },
        "401": {
          "$ref": "../Api.json#/components/responses/401"
        }
      },
      "security": [
        {
          "basicAuth": []
        }
      ]
    }
  },
  "Errors" : {
    "get": {
      "description": "Returns the number of tasks with error-feedback from dispatcher",
      "responses": {
        "200": {
          "$ref": "../Api.json#/components/responses/200_Integer"
        },
        "401": {
          "$ref": "../Api.json#/components/responses/401"
        }
      },
      "security": [
        {
          "basicAuth": []
        }
      ]
    }
  },
  "TasksByGroup" : {
    "get": {
      "description": "Returns the amount of tasks from a group",
      "parameters": [
        {
          "name": "group_Id",
          "in": "path",
          "schema": {
            "type": "integer",
            "format" : "int64"
          },
          "required": true
        }
      ],
      "responses": {
        "200": {
          "$ref": "../Api.json#/components/responses/200_Integer"
        },
        "404" : {
          "description": "404: Group not found",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "errorMessage" : {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "401": {
          "$ref": "../Api.json#/components/responses/401"
        }
      },
      "security": [
        {
          "basicAuth": []
        }
      ]
    }
  },
  "GroupCount" : {
    "get": {
      "description": "Returns the total number of groups",
      "responses": {
        "200": {
          "$ref": "../Api.json#/components/responses/200_Integer"
        },
        "401": {
          "$ref": "../Api.json#/components/responses/401"
        }
      },
      "security": [
        {
          "basicAuth": []
        }
      ]
    }
  }
}